#include <stdio.h>
int main(){
	int i,n,key,flag=0;
	int arr[100];
	printf("n = ");
	scanf("%d",&n);
	for(i=0;i<n;i++){
		printf("a[%d] = ",i);
		scanf("%d",&arr[i]);
		
	}
		
		printf("Search key : ");
		scanf("%d",&key);
		for(i=0;i<n;i++){
			if(key==arr[i]){
			printf("Key %d is found at position %d.\n",key,i);
			flag=1;
			break;
			
		}
	}
if(flag==0){
	printf("Key %d is not found.\n",key);
}
return 0;
}
